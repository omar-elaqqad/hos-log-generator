
{
  "version": 2,
  "builds": [
    {
      "src": "triplog_backend/triplog_backend/wsgi.py",
      "use": "@vercel/python"
    },
    {
      "src": "triplog_frontend/package.json",
      "use": "@vercel/static-build",
      "config": { "distDir": "build" }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "triplog_backend/triplog_backend/wsgi.py"
    },
    {
      "src": "/static/(.*)",
      "dest": "/triplog_frontend/build/static/$1"
    },
    {
      "src": "/(.*\\..*)",
      "dest": "/triplog_frontend/build/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/triplog_frontend/build/index.html"
    }
  ]
}