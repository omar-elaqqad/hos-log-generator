{
  "version": 2,
  "builds": [
    {
      "src": "triplog_backend/triplog_backend/wsgi.py",
      "use": "@vercel/python"
    },
    {
      "src": "triplog_frontend/package.json",
      "use": "@vercel/static-build",
      "config": {
        "outputDirectory": "build"
      }
    }
  ],
  "routes": [
  {
    "src": "/api/(.*)",
    "dest": "triplog_backend/triplog_backend/wsgi.py"
  },
  {
    "src": "/static/(.*)",
    "dest": "/static/$1"
  },
  {
    "src": "/favicon.ico",
    "dest": "/favicon.ico"
  },
  {
    "src": "/manifest.json",
    "dest": "/manifest.json"
  },
  {
    "src": "/(.*)",
    "dest": "/index.html"
  }
]
}